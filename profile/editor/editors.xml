<editors>
    <!-- Boolean -->
    <editor id="bool">
        <range uom="2" subset="0,1" />
    </editor>

    <editor id="DM">
        <range uom="25" subset="0,10,20,30,40,50" nls="CDM" />
    </editor>

    <editor id="ON_OFF">
        <range uom="25" subset="0,1" nls="ON_OFF" />
    </editor>

    <editor id="FLOAT">
       <range uom="56" min="0.1" max="999.999"/>
    </editor>

    <editor id="INTEGER">
      <range uom="56" min="0" max="4294967295" prec="0" />
    </editor>

    <editor id ="HB">
        <range uom="25" subset="0,1" nls="HB"/>
    </editor>

    <editor id="DEVICE_TYPE">
        <range uom="25" min="0" nls="DTYPE"/>
    </editor>

    <editor id="VOLUME">
        <range uom="56" min="-80" max="12" step=".5" prec="1"/>
    </editor>

    <editor id="SOURCE">
        <range uom="25" subset="0,1,2,3,4,5,10,11,14,15,17,18,19,20,21,22,23,25,26,27,31,33" nls="SOURCE"/>
    </editor>
</editors>
