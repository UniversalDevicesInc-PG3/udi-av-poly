<editors>
    <!-- Boolean -->
    <editor id="bool">
        <range uom="2" subset="0,1" />
    </editor>

    <editor id="I_DM">
        <range uom="25" subset="0,10,20,30,40,50" nls="CDM" />
    </editor>

    <editor id="I_ON_OFF">
        <range uom="25" subset="0,1" nls="ON_OFF" />
    </editor>

    <editor id="I_FLOAT">
       <range uom="56" min="0.1" max="999.999" prec="3"/>
    </editor>

    <editor id="I_INTEGER">
      <range uom="56" min="0" max="4294967295" prec="0" />
    </editor>

    <editor id ="I_HB">
        <range uom="25" subset="0,1" nls="HB"/>
    </editor>

    <editor id="I_DEVICE_TYPE">
        <range uom="25" subset="0,1,2" nls="DTYPE"/>
    </editor>

    <editor id="I_VOLUME">
        <range uom="56" min="0" max="100" step="1" prec="0"/>
    </editor>

    <editor id="I_VSX1021_DB_VOLUME_LIST">
        <range uom="13" min="-80" max="12" step=".5" prec="1"/>
    </editor>

    <editor id="I_VSX1021_DB_VOLUME_FLOAT">
       <range uom="56" prec="1"/>
    </editor>

    <editor id="I_SOURCE">
        <range uom="25" subset="999" nls="SOURCE"/>
    </editor>

    <editor id="I_VSX1021_SOURCE">
        <range uom="25" subset="0,1,2,3,4,5,10,11,14,15,17,18,19,20,21,22,23,25,26,27,31,33,999" nls="VSX1021_SOURCE"/>
    </editor>

    <editor id="I_BRAVIA_SOURCE">
        <range uom="25" subset="0-6,999" nls="BRAVIA_SOURCE"/>
    </editor>
</editors>
